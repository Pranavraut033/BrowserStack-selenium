    "start": "set NODE_ENV=production & node dist/app.js",
    "serve": "set NODE_ENV=development & babel-node src/app.js"
  },
  "dependencies": {
    "selenium-webdriver": "^4.0.0-alpha.7",
    "core-js": "^3.6.5",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "nodemon": "^2.0.4"
  }
}
