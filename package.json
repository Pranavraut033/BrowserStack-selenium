{
  "name": "flipkart-selenium",
  "version": "1.0.0",
  "description": "Browserstack round 1 sub 3",
  "main": "dist/app.js",
  "author": "Pranav ",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "set NODE_ENV=testing & mocha --require @babel/register './src/test/*.test.js'",
